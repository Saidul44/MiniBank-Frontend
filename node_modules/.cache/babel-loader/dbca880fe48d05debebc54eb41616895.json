{{ item.heading }}</v-subheader>\n            </v-flex>\n            <!-- <v-flex xs6 class=\"text-xs-center\">\n              <a href=\"#!\" class=\"body-2 black--text\">EDIT</a>\n            </v-flex>-->\n          </v-layout>\n          <v-list-group\n            v-else-if=\"item.children\"\n            :key=\"item.text\"\n            v-model=\"item.model\"\n            :prepend-icon=\"item.model ? item.icon : item['icon-alt']\"\n            append-icon\n          >\n            <template v-slot:activator>\n              <v-list-tile>\n                <v-list-tile-content>\n                  <v-list-tile-title>{{ item.text }}</v-list-tile-title>\n                </v-list-tile-content>\n              </v-list-tile>\n            </template>\n            <v-list-tile v-for=\"(child, i) in item.children\" :key=\"i\" router :to=\"child.route\">\n              <v-list-tile-action v-if=\"child.icon\">\n                <v-icon>{{ child.icon }}</v-icon>\n              </v-list-tile-action>\n              <v-list-tile-content>\n                <v-list-tile-title>{{ child.text }}</v-list-tile-title>\n              </v-list-tile-content>\n            </v-list-tile>\n          </v-list-group>\n          <v-list-tile v-else :key=\"item.text\" router :to=\"item.route\">\n            <v-list-tile-action>\n              <v-icon>{{ item.icon }}</v-icon>\n            </v-list-tile-action>\n            <v-list-tile-content>\n              <v-list-tile-title>{{ item.text }}</v-list-tile-title>\n            </v-list-tile-content>\n          </v-list-tile>\n        </template>\n      </v-list>\n    </v-navigation-drawer>\n    <v-toolbar :clipped-left=\"$vuetify.breakpoint.lgAndUp\" class=\"navbar\" color=\"white\" app fixed>\n      <v-toolbar-title style=\"width: 200px\" class=\"grey--text ml-0 pl-3\" color=\"grey\">\n        <v-toolbar-side-icon\n          v-if=\"loggedIn\"\n          @click.stop=\"drawer = !drawer\"\n          class=\"navigationDrawer\"\n        ></v-toolbar-side-ico