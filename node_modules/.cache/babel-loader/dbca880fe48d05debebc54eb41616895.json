nup\">\n        <span>Sign up</span>\n      </v-btn>\n      <div class=\"dropdown\">\n        <button class=\"dropbtn\" style=\"color:#0000A0;\" v-if=\"loggedIn\">\n          {{ userInfo.name}}\n          <v-icon>arrow_drop_down</v-icon>\n        </button>\n        <div class=\"dropdown-content\">\n          <router-link to=\"/profile/profile\">Profile</router-link>\n          <router-link to=\"/signout\">Logout</router-link>\n        </div>\n      </div>\n      <!-- <div class=\"text-xs-center\">\n        <v-menu open-on-hover light left offset-y v-if=\"loggedIn\">\n          <template v-slot:activator=\"{ on }\">\n            <v-btn flat class=\"blue--text\" v-on=\"on\">\n              {{ userInfo.name}}\n              <v-icon>arrow_drop_down</v-icon>\n            </v-btn>\n          </template>\n          <v-list class=\"dropDownMenu\">\n            <v-list-tile v-for=\"(item, index) in items\" :key=\"index\" router :to=\"item.route\">\n              <v-list-tile-title>{{ item.title }}</v-list-tile-title>\n            </v-list-tile>\n          </v-list>\n        </v-menu>\n      </div>-->\n      <!-- <v-btn flat outline round v-if=\"loggedIn\" to=\"/signout\">\n        <span>Sign out</span>\n      </v-btn>-->\n    </v-toolbar>\n    <v-img src=\"../navbarShadow.png\" class=\"header_shadow\"></v-img>\n  </nav>\n</template>\n\n<script>\nexport default {\n  data: () => ({\n    dialog: false,\n    drawer: false,\n    items: [\n      { title: \"Profile\", route: \"/\" },\n      { title: \"Sign Out\", route: \"/signout\" }\n    ]\n  }),\n  props: {\n    source: String\n  },\n  created() {},\n  computed: {\n    loggedIn() {\n      return this.$store.getters.loggedIn;\n    },\n    userInfo() {\n      console.log(this.$store.getters.getUserInfo);\n      return this.$store.getters.getUserInfo;\n    },\n    getNavList() {\n      if (this.userInfo.user_type == \"hospital\") {\n        return [\n          { icon: \"dashboard\", text: \"Dashboard\", route: \"/\" },\n          {\n            icon: \"keyboard_arrow_up\",\n            \"icon-alt\": \"keyboard_arrow_down\",\n            text: \"List Items\",\n            model: true,\n            children: [\n              {\n                icon: \"list\",\n                text: \"Patients List\",\n                route: \"/patients/patientsList\"\n              },\n              {\n                icon: \"list\",\n                text: \"Doctors List\",\n                route: \"/doctors/doctorsList\"\n              },\n              {\n                icon: \"list\",\n                text: \"Medicines List\",\n                route: \"/medicines/medicinesList\"\n              },\n              { icon: \"list\", text: \"Test List\", route: \"/tests/testsList\" }\n            ]\n          },\n          {\n            icon: \"assignment_ind\",\n            text: \"Patients Report\",\n            route: \"/patientsReport/report\"\n          },\n          {\n            icon: \"assignment_ind\",\n            text: \"Patients Report By Date\",\n            route: \"/patientsReport/reportByDate\"\n          },\n          { icon: \"person\", text: \"Add Member\", route: \"/members\" },\n          {\n            icon: \"settings\",\n            text: \"Time Slot Settings\",\n            route: \"/settings/settings/time-slot\"\n          }\n        ];\n      } else if (this.userInfo.user_type == \"doctor\") {\n        return [\n          { icon: \"dashboard\", text: \"Dashboard\", route: \"/\" },\n          {\n            icon: \"keyboard_arrow_up\",\n            \"icon-alt\": \"keyboard_arrow_down\",\n            text: \"List Items\",\n            model: true,\n            children: [\n              {\n                icon: \"list\",\n                text: \"Patients List\",\n                route: \"/patients/patientsList\"\n              },\n              {\n                icon: \"list\",\n                text: \"Doctors List\",\n                route: \"/doctors/doctorsList\"\n              },\n              {\n                icon: \"list\",\n                text: \"Medicines List\",\n                route: \"/medicines/medicinesList\"\n              },\n              { icon: \"list\", text: \"Test List\", route: \"/tests/testsList\" }\n            ]\n          },\n          {\n            icon: \"assignment_ind\",\n            text: \"Patients Report\",\n            route: \"/patientsReport/report\"\n          },\n          {\n            icon: \"assignment_ind\",\n            text: \"Patients Report By Date\",\n            route: \"/patientsReport/reportByDate\"\n          },\n          { icon: \"person\", text: \"Add Member\", route: \"/members\" },\n          {\n            icon: \"settings\",\n            text: \"Time Slot Settings\",\n            route: \"/settings/settings/time-slot\"\n          },\n          {\n            icon: \"settings\",\n            text: \"Prescription Header Settings\",\n            route: \"/settings/settings/header\"\n          }\n        ];\n      } else {\n        return [\n          { icon: \"dashboard\", text: \"Dashboard\", route: \"/\" },\n          {\n            icon: \"keyboard_arrow_up\",\n            \"icon-alt\": \"keyboard_arrow_down\",\n            text: \"List Items\",\n            model: true,\n            children: [\n              {\n                icon: \"list\",\n                text: \"Patients List\",\n                route: \"/patients/patientsList\"\n              }\n            ]\n          },\n          {\n            icon: \"assignment_ind\",\n            text: \"Patients Report\",\n            route: \"/patientsReport/report\"\n          },\n          {\n            icon: \"assignment_ind\",\n            text: \"Patients Report By Date\",\n            route: \"/patientsReport/reportByDate\"\n          }\n        ];\n      }\n    }\n  },\n  methods: {}\n};\n</script>\n\n\n<style>\n.v-toolbar {\n  height: 60px;\n  box-shadow: 0px 1px 0px -1px #d3e0e9, 0px 1px 0px 0px #d3e0e9,\n    0px 1px 0px 