     {{ getPrintHeaderDetails.address}}\n          </p>\n        </v-flex>\n        <v-flex xs4 sm4 md4>\n          <v-img src=\"../square.png\" class=\"ml-4 mt-4\" id=\"img\"></v-img>\n        </v-flex>\n        <v-flex xs4 sm4 md4>\n          <h2 class=\"heading green--text text-lg-right\">{{ getPrintHeaderDetails.doctor_name}}</h2>\n          <p class=\"text-lg-right\">{{ getPrintHeaderDetails.degree}}</p>\n        </v-flex>\n      </v-layout>\n      <v-layout :style=\"headerSpace\" v-else></v-layout>\n      <hr />\n      <br />\n      <v-layout row wrap class=\"patientHead\">\n        <v-flex xs4 sm4 md4 class=\"headeritem\">\n          Name:\n          <br />\n          <h4>\n            <span>{{ desserts.name }}</span>\n          </h4>\n        </v-flex>\n        <v-flex xs4 sm4 md2 class=\"headeritem\">\n          Age:\n          <br />\n          <h4>\n            <span>{{ desserts.age }}</span>\n          </h4>\n        </v-flex>\n        <v-flex xs4 sm4 md3 class=\"headeritem\">\n          Phone No:\n          <br />\n          <h4>\n            <span>{{ desserts.contact }}</span>\n          </h4>\n        </v-flex>\n        <v-flex xs4 sm4 md2 class=\"headeritem\">\n          Sex:\n          <br />\n          <h4>\n            <span>{{ desserts.gender }}</span>\n          </h4>\n        </v-flex>\n      </v-layout>\n      <br />\n      <v-layout row wrap class=\"mb-4\">\n        <v-flex xs4 sm4 md4 class=\"left\">\n          <v-layout column>\n            <v-flex xs12 sm6 md6 class=\"symptomp\">\n              <h2 class=\"subheading grey--text ml-2\">symptoms</h2>\n              <v-list dense class=\"ml-5\">\n                <v-list-tile v-for=\"(symptom,index) in symptoms\" :key=\"symptom\">\n                  <v-list-tile-content>\n                    <v-list-tile-title>\n                      <span>{{++index}}.</span>\n                      {{ symptom }}\n                    </v-list-tile-title>\n                  </v-list-tile-content>\n                </v-list-tile>\n              </v-list>\n            </v-flex>\n            <v-flex xs12 sm6 md6 class=\"test\">\n              <h2 class=\"subheading grey--text ml-2\">Tests</h2>\n              <v-list dense class=\"ml-5\">\n                <v-list-tile v-for=\"(test,index) in tests\" :key=\"test.test_name\">\n                  <v-list-tile-content>\n                    <v-list-tile-title>\n                      <span>{{++index}}.</span>\n                      {{ test.test_name }}\n                    </v-list-tile-title>\n                  </v-list-tile-content>\n                </v-list-tile>\n              </v-list>\n            </v-flex>\n          </v-layout>\n        </v-flex>\n        <v-flex xs8 sm8 md8 class=\"right\">\n          <h2 class=\"subheading grey--text ml-2\">Medicines</h2>\n          <v-list dense class=\"ml-5\">\n            <v-list-tile v-for=\"(medicine,index) in medicines\" :key=\"medicine.medicine_name\">\n              <v-list-tile-content>\n                <v-list-tile-title>\n                  <span>{{++index}}.</span>\n                  {{ medicine.medicine_name}}\n                </v-list-tile-title>\n                <v-list-tile-sub-title\n                  class=\"ml-3\"\n                >{{medicine.schedule +\" (\"+medicine.duration+\" days)\"}}</v-list-tile-sub-title>\n              </v-list-tile-content>\n            </v-list-tile>\n          </v-list>\n        </v-flex>\n      </v-layout>\n      <v-layout class=\"suggestions mt-5\">\n        <v-flex>\n          <h4>suggestion</h4>\n          <p class=\"paragraph\">{{ suggestions }}</p>\n        </v-flex>\n      </v-layout>\n    </v-container>\n  </div>\n</template>\n\n<script>\nexport default {\n  created() {\n    this.fetchData();\n    this.getPrintHeader();\n  },\n  computed: {\n    desserts() {\n      return this.$store.getters.getPrescriptionHead;\n    },\n    symptoms() {\n      return this.$store.state.symptomsPrescription;\n    },\n    medicines() {\n      return this.$store.state.medicinesPrescription;\n    },\n    tests() {\n      return this.$store.state.testsPrescription;\n    },\n    suggestions() {\n      return this.$store.state.suggestionsPrescription;\n    },\n    headerType() {\n      return this.$store.state.headerType;\n    },\n    getPrintHeaderDetails() {\n      return this.$store.getters.getPrintHeaderDetails;\n    },\n    headerSpace() {\n      if (this.headerType == \"withoutHeader\") {\n        return {\n          height: this.getPrintHeaderDetails.header + \"cm\"\n        };\n      }\n    }\n  },\n  methods: {\n    getPrintHeader() {\n      this.$store.dispatch(\"getPrintHeaderDetails\");\n    },\n    printPrescription(divName) {\n      var printContents = document.getElementById(divName).innerHTML;\n      var originalContents = document.body.innerHTML;\n\n      document.body.innerHTML = printContents;\n\n      window.print();\n\n      document.body.innerHTML = originalContents;\n    },\n    fetchData() {\n      this.$store.dispatch(\"getPrescription\", this.$route.params.id);\n    }\n  }\n};\n</script>\n\n\n<style>\n.headeritem {\n  border: 2px solid gray;\n  border-radius: 10px;\n  padding: 10px 0px;\n  margin-right: 18px;\n}\n.headeritem h4 span {\n  margin-left: 10px;\n}\n.left {\n  border-right: 2px solid gray;\n}\n\n.symptomp {\n  border-bottom: 2px solid gray;\n}\n\n@media print {\n  #print,\n  #print * {\n    visibility: hidden;\n  }\n  #print {\n    position: absolute;\n    left: 0;\n    top: 0;\n  }\n  #printableArea {\n    position: absolute;\n    left: 0;\n    top: 0;\n  }\n}\n\n#img {\n  width: 55%;\n  height: auto;\n}\n.paragraph {\n  margin-top: 10px;\n  border: 2px solid gray;\n  padding-block-end: 5px;\n  padding: 5px;\n}\n</style>"],"sourceRoot":"src/views/prescriptions"}]}