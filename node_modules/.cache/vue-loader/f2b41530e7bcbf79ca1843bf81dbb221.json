 </v-list-tile>\n            </template>\n            <v-list-tile v-for=\"(child, i) in item.children\" :key=\"i\" router :to=\"child.route\">\n              <v-list-tile-action v-if=\"child.icon\">\n                <v-icon>{{ child.icon }}</v-icon>\n              </v-list-tile-action>\n              <v-list-tile-content>\n                <v-list-tile-title>{{ child.text }}</v-list-tile-title>\n              </v-list-tile-content>\n            </v-list-tile>\n          </v-list-group>\n          <v-list-tile v-else :key=\"item.text\" router :to=\"item.route\">\n            <v-list-tile-action>\n              <v-icon>{{ item.icon }}</v-icon>\n            </v-list-tile-action>\n            <v-list-tile-content>\n              <v-list-tile-title>{{ item.text }}</v-list-tile-title>\n            </v-list-tile-content>\n          </v-list-tile>\n        </template>\n      </v-list>\n    </v-navigation-drawer>\n    <v-toolbar :clipped-left=\"$vuetify.breakpoint.lgAndUp\" class=\"navbar\" color=\"white\" app fixed>\n      <v-toolbar-title style=\"width: 200px\" class=\"grey--text ml-0 pl-3\" color=\"grey\">\n        <v-toolbar-side-icon\n          v-if=\"loggedIn\"\n          @click.stop=\"drawer = !drawer\"\n          class=\"navigationDrawer\"\n        ></v-toolbar-side-icon>\n        <span class=\"hidden-sm-and-down\">Doctors Diary</span>\n      </v-toolbar-title>\n      <div class=\"Menu\" v-if=\"loggedIn\">\n        <div class=\"dropdown\">\n          <button class=\"dropbtn\">\n            List items\n            <v-icon>arrow_drop_down</v-icon>\n          </button>\n          <div class=\"dropdown-content\">\n            <router-link to=\"/patients/patientsList\">Patients List</router-link>\n            <router-link to=\"/medicines/medicinesList\">Medicines List</router-link>\n            <router-link to=\"/tests/testsList\">Tests List</router-link>\n          </div>\n        </div>\n        <div class=\"dropdown\">\n          <button class=\"dropbtn\">\n           